{
    "name": "cundd/composer",
    "description": "Composer for TYPO3 CMS",
    "keywords": [
        "typo3cms"
    ],
    "type": "typo3-cms-extension",
    "license": "MIT",
    "homepage": "https://github.com/cundd/CunddComposer",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net",
            "homepage": "http://cundd.net/"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\CunddComposer\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\CunddComposer\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "cundd_composer": "*",
        "cundd.composer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    }
}
