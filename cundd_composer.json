{
    "name": "cundd/composer",
    "description": "Composer for TYPO3 CMS",
    "keywords": [
        "typo3cms"
    ],
    "type": "typo3-cms-extension",
    "license": "MIT",
    "homepage": "https://github.com/cundd/cundd_composer",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net",
            "homepage": "http://cundd.net/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\CunddComposer\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\CunddComposer\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "cundd_composer": "*",
        "cundd.composer": "*"
    },
    "suggest": {
        "phpunit/phpunit": "To run the included Unit tests (PHPUnit 6 & PHP 7 required)"
    }
}
